<h1>部屋登録</h1>
    
    <%= form_with model: @room, local: true do |f| %>
      <table>
        <tr>
          <th><%= f.label :room_name ,"名前" %></th>
          <td><%= f.text_field :room_name %></td>
        </tr>
 
        <tr>
          <th><%= f.label :introduction ,"情報" %></th>
          <td><%= f.text_field :introduction %></td>
        </tr>
 
        <tr>
          <th><%= f.label :room_price ,"料金" %></th>
          <td><%= f.text_field :room_price %></td>
        </tr>
      </table>
 
      <div>
        <ul>
          <li><%= f.submit "登録完了する"%></li>
          <li><%= link_to "ユーザー情報一覧に戻る", :rooms %></li>
        </ul>
      </div>
    <% end %>

<%= link_to '戻る', rooms_path %>