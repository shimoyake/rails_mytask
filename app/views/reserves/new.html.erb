<h1>今使わない</h1>
    
    <%= form_with model: @reserve, url:reserves_path, local: true do |f| %> <!--ルーティングエラー：url:飛ばしたいパス-->
      <table>
        <tr>
          <th><%= f.label :begin ,"開始日" %></th>
          <td><%= f.date_field :begin, min: Date.current%></td>
        </tr>
 
        <tr>
          <th><%= f.label :end ,"終了日" %></th>
          <td><%= f.date_field :end, min: Date.current%></td>
        </tr>
 
        <tr>
          <th><%= f.label :number_of_peple ,"人数" %></th>
          <td><%= f.number_field :number_of_peple %></td>
        </tr>
      </table>
 
      <div>
        <ul>
          <li><%= f.submit "登録完了する"%></li>
          <li><%= link_to "予約済ルーム情報一覧に戻る", :rooms %></li>
        </ul>
      </div>
    <% end %>

<%= link_to '戻る', :back %>